<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="lprod">

  <select id="selectprod" resultType="LprodVO" parameterType="String">
  	select * 
  	from lprod
  	where lprod_gu = #{sss}
  </select>
  
  <select id="allprod" resultType="LprodVO">
  	select * 
  	from lprod
  </select>
  
</mapper>